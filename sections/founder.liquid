<div class="{{ section.settings.section_css_class }}">
<div class="founder_desc">
    <h2>{{ section.settings.heading }}</h2>
    <div class="founder_img"> <img src='{{ section.settings.Image | img_url: ' ' }}'></div>
    <div class="wpb_wrapper">
        <h3>{{ section.settings.subheading }}</h3>
         <p>{{ section.settings.text }}</p>
        <h3>{{ section.settings.label }}</h3>

    </div>
</div>
</div>

